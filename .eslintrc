{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [],
  "ecmaFeatures": {
    "jsx": false,
    "modules": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
  },
  "rules": {
    "comma-dangle": 0,
    "quotes": [2, "single"],
    "eol-last": 2,
    "no-debugger": 1,
    "no-mixed-requires": 0,
    "no-underscore-dangle": 0,
    "no-multi-spaces": 0,
    "no-trailing-spaces": 0,
    "no-extra-boolean-cast": 0,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-var": 2,
    "no-param-reassign": 0,
    "no-else-return": 0,
    "no-console": 0,
    "prefer-const": 2,
    "new-cap": 0,
    "camelcase": 0,
    "semi": 0,
    "valid-jsdoc": ["error"],
    "indent": 0,
    "max-len": 0,
    "dot-notation": 0,
    "quote-props": 0,
    "object-shorthand": 0,
    "brace-style": 0,
  }
}
